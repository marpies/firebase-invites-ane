<extension xmlns="http://ns.adobe.com/air/extension/20.0">
  <id>com.marpies.ane.firebase.invites</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libFirebaseInvites.a</nativeLibrary>
            <initializer>FirebaseInvitesInitializer</initializer>
            <finalizer>FirebaseInvitesFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libFirebaseInvites.jar</nativeLibrary>
            <initializer>com.marpies.ane.firebase.invites.FirebaseInvitesExtension</initializer>
            <finalizer>com.marpies.ane.firebase.invites.FirebaseInvitesExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-x86">
        <applicationDeployment>
            <nativeLibrary>libFirebaseInvites.jar</nativeLibrary>
            <initializer>com.marpies.ane.firebase.invites.FirebaseInvitesExtension</initializer>
            <finalizer>com.marpies.ane.firebase.invites.FirebaseInvitesExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>
  </platforms>
</extension>
